<ViewModels:DjHorsifyViewModel xmlns:ViewModels="clr-namespace:Horsesoft.Horsify.DjHorsify.ViewModels" 
                               xmlns:Model="clr-namespace:Horsesoft.Horsify.DjHorsify.Model"
                               xmlns:System="clr-namespace:System;assembly=mscorlib">
    
    <ViewModels:DjHorsifyViewModel.DjHorsifyOption>
        <Model:DjHorsifyOption Amount="9" HarmonicEnabled="35" IsEnabled="True" SelectedKeys="F"/>
    </ViewModels:DjHorsifyViewModel.DjHorsifyOption>

    <ViewModels:DjHorsifyViewModel.HorsifyFilters>
        
        <Model:DjHorsifyFilterModel FileName="Jungle" Id="57" SearchType="Genre" SearchAndOrOption="None">
            <Model:DjHorsifyFilterModel.Filters>
                <System:String>%Drum & bass, Jungle%</System:String>
                <System:String>%Jungle</System:String>
            </Model:DjHorsifyFilterModel.Filters>
        </Model:DjHorsifyFilterModel>

        <Model:DjHorsifyFilterModel FileName="Boogie" Id="58" SearchType="Genre" SearchAndOrOption="None">
            <Model:DjHorsifyFilterModel.Filters>
                <System:String>%Boogie</System:String>
                <System:String>%Disco,%</System:String>
            </Model:DjHorsifyFilterModel.Filters>
        </Model:DjHorsifyFilterModel>
        
    </ViewModels:DjHorsifyViewModel.HorsifyFilters>

</ViewModels:DjHorsifyViewModel>
