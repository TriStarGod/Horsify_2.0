<ViewModels:SearchedSongsViewModel xmlns:ViewModels="clr-namespace:Horsesoft.Horsify.SearchModule.ViewModels" 
                                   xmlns:Commands="clr-namespace:Prism.Commands;assembly=Prism" 
                                   xmlns:Model="clr-namespace:Horsesoft.Horsify.SearchModule.Model"
                                   Amount="12" RatingUpper="5" RatingLower="4">

    <ViewModels:SearchedSongsViewModel.GetRandomSongsCommand>
        <Commands:DelegateCommand IsActive="True"/>
    </ViewModels:SearchedSongsViewModel.GetRandomSongsCommand>

    <ViewModels:SearchedSongsViewModel.RecentSearch>
        <Model:RecentSearch ResultCount="28" SearchTerm="%Last Search%"/>
    </ViewModels:SearchedSongsViewModel.RecentSearch>

</ViewModels:SearchedSongsViewModel>


